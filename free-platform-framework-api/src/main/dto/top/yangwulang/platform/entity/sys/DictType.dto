import jakarta.validation.constraints.NotEmpty

DictTypeListView {
    #allScalars
}

specification DictTypeListSpecification {
    like(dictType)
    like(dictName)
    isSys
    like(remarks)
}

unsafe input DictSaveInput {
    @NotEmpty(message = "字典类型不能为空")
    dictType
    @NotEmpty(message = "字典名称不能为空")
    dictName
    isSys
    remarks
}

DictTypeGetView {
    #allScalars
    -status
    -createBy
    -createDate
    -updateBy
    -updateDate
}

